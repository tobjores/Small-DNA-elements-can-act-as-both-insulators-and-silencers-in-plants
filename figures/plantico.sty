\newcommand{\plic@protoplast}[1]{
	\coordinate (pp@center) at (#1);
	\begin{scope}[scale = \plic@scale, rotate around = {\plic@rotate:(pp@center)}]
		\useasboundingbox (pp@center) +(1, 1) rectangle +(-1, -1);
		\shadedraw[plic@pp@membrane, outer color = plic@pp@cytosol, inner color = white, line width = \plic@scale * 1pt] (pp@center) circle (1cm - 0.5pt);
		\begin{scope}
			\clip (pp@center) circle (1cm - 1pt);	
			\foreach \i in {1, ..., 20} {
				\coordinate[shift = {(\plic@scale * rand * 1cm, \plic@scale * rand * 1cm)}] (cp\i) at (pp@center);
				\filldraw[
					plic@pp@chloroplast,
					line width = \plic@scale * 0.8pt,
					fill opacity = 0.25 + rnd * 0.25,
					draw opacity = 0.5 + rnd * 0.5,
					rotate = rand * 360,
					x radius = 0.25 + rand * 0.075,
					y radius = 0.15 + rand * 0.05
				] (cp\i) ellipse;
			}
		\end{scope}
	\end{scope}
}

