%%% load required packages
\RequirePackage{xcolor}
\RequirePackage{tikz}
\usetikzlibrary{calc, decorations.pathmorphing, patterns.meta}


%%% set up macros and booleans 
\newif\ifplic@special

\def\plic@scale{1}
\def\plic@rotate{0}
\def\plic@width{}
\def\plic@height{}


%%% define options
\def\plic@set#1{\pgfqkeys{/plic}{#1}}

\plic@set{
	scale/.store in = \plic@scale,
	rotate/.store in = \plic@rotate,
	width/.code = {\pgfmathsetmacro{\plic@scale}{#1 / \plic@width}},
	height/.code = {\pgfmathsetmacro{\plic@scale}{#1 / \plic@height}},
	size/.is choice
}


%%% define aliases
% use:	\plantalias{<plant name>}{<alias>}
\newcommand{\plantalias}[2]{%
	\ifcsname plic@#1\endcsname% test if a command for <plant name> exists
		\ifcsname plic@#2\endcsname% test if a command for <alias> already exists
			\PackageError{plantico}{The plant/alias "#2" is already defined}{}
		\else
			\expandafter\def\csname plic@#2\endcsname{\csname plic@#1\endcsname}%
			\plic@set{size/#2/.style = {size/#1}}
		\fi
	\else
		\PackageError{plantico}{Unknown plant: #1}{}%
	\fi
}


%%% command to draw plant icons
% use:	\planticon[<options>]{<plant name>} at (<coordinate>);
\def\planticon{%
	\@ifstar{\plic@specialtrue\planticon@}{\plic@specialfalse\planticon@}%
}
\def\planticon@{%
	\@ifnextchar[{\planticon@@}{\planticon@@[]}%
}
\def\planticon@@[#1]#2 at (#3);{%
		\ifcsname plic@#2\endcsname% test if a command for <plant name> exists
			\begingroup
				\plic@set{size = #2, #1}
				\csname plic@#2\endcsname{#3}
			\endgroup
		\else%
			\PackageError{plantico}{Unknown plant: #2}{}%
		\fi%
}


